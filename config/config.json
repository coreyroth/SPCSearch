{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "spc-search-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spcSearch/SpcSearchWebPart.js",
          "manifest": "./src/webparts/spcSearch/SpcSearchWebPart.manifest.json"
        }
      ]
    },
    "product-search-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/productSearch/ProductSearchWebPart.js",
          "manifest": "./src/webparts/productSearch/ProductSearchWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SpcSearchWebPartStrings": "lib/webparts/spcSearch/loc/{locale}.js",
    "ProductSearchWebPartStrings": "lib/webparts/productSearch/loc/{locale}.js"
  }
}
